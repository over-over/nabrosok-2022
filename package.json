  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "static": "next build && next export -o docs && touch ./docs/.nojekyll",
    "sheet:get": "yarn ts-node -T -P node.tsconfig.json scripts/spreadsheet-data/fetch-spreadsheet-data.ts",
    "sheet:artists": "yarn ts-node -T -P node.tsconfig.json scripts/spreadsheet-data/format-artist-data.ts",
    "sheet:images": "yarn ts-node -T -P node.tsconfig.json scripts/spreadsheet-data/fetch-images.ts",
    "sheet:tg": "yarn ts-node -T -P node.tsconfig.json scripts/spreadsheet-data/post-auction-channel.ts",
    "sheet:size": "yarn ts-node -T -P node.tsconfig.json scripts/spreadsheet-data/resize-images.ts",
    "sheet:price": "yarn ts-node -T -P node.tsconfig.json scripts/spreadsheet-data/update-prices.ts"
  },
  "dependencies": {
    "next": "12.1.6",
    "qrcode.react": "^3.0.1",
    "react": "18.1.0",
