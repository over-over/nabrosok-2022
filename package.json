    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/sharp": "^0.30.2",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "sharp": "^0.30.4",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4"
  }
